{
  "name": "@nuxtify/core",
  "version": "0.1.10",
  "description": "Nuxtify core module powered by Nuxt and Vuetify.",
  "homepage": "https://nuxtify.dev",
  "author": "Nuxtify.dev <hello@nuxtify.dev>",
  "funding": "https://github.com/sponsors/davidstackio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxtify-dev/core"
  },
  "bugs": {
    "url": "https://github.com/nuxtify-dev/core/issues"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "defu": "^6.1.4",
    "vuetify-nuxt-module": "^0.19.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.7.0",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/eslint-config": "^1.3.1",
    "@nuxt/kit": "^3.20.2",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^3.20.2",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "^22.19.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^3.20.2",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
